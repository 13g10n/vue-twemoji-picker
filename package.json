{
  "name": "cool-emoji-picker",
  "version": "3.0.1",
  "main": "./index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:kevinfaguiar/cool-emoji-picker.git"
  },
  "keywords": [
    "emoji-data",
    "twemoji",
    "emoji-picker",
    "emoji-textarea",
    "sanitize-textarea",
    "vue"
  ],
  "scripts": {
    "serve:app": "vue-cli-service serve",
    "build:emoji-data": "node ./src/utils/remove-unused-with-tags.js",
    "build:app": "vue-cli-service build --mode production --dest docs",
    "build:test": "vue-cli-service build --mode build-test",
    "build:comp": "vue-cli-service build --target lib --name cool-emoji-picker ./index.js",
    "publish:gh-pages": "git subtree split --prefix docs -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages",
    "publish:npm": "npm publish --access public",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "twemoji": "^12.0.4",
    "v-tooltip": "^2.0.2",
    "vue-dragscroll": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-vue": "^2.0.2",
    "emojibase-data": "^4.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-burger-menu": "^2.0.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
