{
  "name": "cool-emoji-picker",
  "version": "3.2.0",
  "main": "./dist/cool-emoji-picker.common.js",
  "unpkg": "./dist/cool-emoji-picker.umd.min.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "browser": {
    "./coolpicker": "./src/components/coolpicker",
    "./cooltextarea": "./src/components/cooltextarea",
    "./emoji-data": "./src/emoji-data",
    "./services": "./src/services"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:kevinfaguiar/cool-emoji-picker.git"
  },
  "keywords": [
    "emoji-data",
    "twemoji",
    "emoji-picker",
    "emoji-textarea",
    "sanitize-textarea",
    "vue"
  ],
  "scripts": {
    "serve:app": "vue-cli-service serve",
    "build:emoji-data": "node ./src/utils/remove-unused-with-tags.js",
    "build:app": "vue-cli-service build --mode production --dest docs",
    "build:test": "vue-cli-service build --mode build-test",
    "build:comp": "vue-cli-service build --target lib --name cool-emoji-picker ./entry.js",
    "publish:gh-pages": "git subtree split --prefix docs -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages",
    "publish:npm": "npm publish --access public",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "twemoji": "^12.1.2",
    "v-tooltip": "^2.0.2"
  },
  "devDependencies": {
    "cool-emoji-picker": "^3.1.6",
    "@babel/core": "^7.6.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "emojibase-data": "^4.0.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-burger-menu": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
